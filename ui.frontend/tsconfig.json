{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "../ui.frontend",
    "removeComments": true,
    "allowJs": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "./src/**/*.ts"
  ]
}
